# Supabase Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Development (optional - defaults to local Supabase)
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your-local-anon-key-here

# Staging/Production (required)
# Get these from your Supabase project dashboard
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_ANON_KEY=your-production-anon-key-here

# Additional configuration
APP_ENV=development  # Options: development, staging, production
